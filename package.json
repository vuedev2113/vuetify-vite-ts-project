    "eslint-plugin-vue": "^8.1.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "sass": "^1.43.5",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-eslint": "^1.3.0",
    "vue-tsc": "^0.29.6"
