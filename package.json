{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vuetify-ts-starter",
  "description": "Vue3 Vuetify TypeScript Startar project for Vite.",
  "version": "1.12.1",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vuetify-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vuetify-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vuetify-ts-starter/issues"
  },
  "engines": {
    "node": ">=22.5.4",
    "pnpm": ">=9.10.0"
  },
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "core-js": "^3.38.1",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.1",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.1",
    "@eslint/js": "^9.10.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.5",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.10.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-vuetify": "^2.4.0",
    "eslint-plugin-yaml": "^1.0.3",
    "husky": "^9.1.6",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.3",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.78.0",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.6",
    "vite-plugin-checker": "0.8.0",
    "vite-plugin-vuetify": "^2.0.4",
    "vite-plugin-vue-devtools": "^7.4.5",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "braces": ">=3.0.3",
    "get-func-name": ">=2.0.2",
    "ip": ">=2.0.1",
    "json5": ">=2.2.3",
    "lodash": ">=4.17.21",
    "postcss": ">=8.4.31",
    "semver": ">=7.5.3",
    "tar": ">=6.2.1",
    "vite": ">=5.0.12",
    "ws": ">=8.17.1",
    "yaml": ">=2.3.2"
  },
  "stackblitz": {
    "startCommand": "pnpm run test:unit"
  }
}
