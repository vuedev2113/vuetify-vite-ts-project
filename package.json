  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint --fix src/*.{js,json,ts,vue} && eslint --fix src/**/*.{js,json,ts,vue}"
