{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vuetify-ts-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --fix src/*.{js,json,ts,vue} && eslint --fix src/**/*.{js,json,ts,vue}",
