{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vuetify-ts-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "vite build",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache && prettier . --write",
    "lint:style": "stylelint **/*.{css,sass,scss,htm,html,vue} --fix --cache",
    "lint:markup": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdi/font": "6.5.95",
    "roboto-fontface": "*",
    "vue": "^3.2.30",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-router": "^4.0.12",
    "vuetify": "^3.0.0-alpha.0",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.2.0",
    "@types/node": "^17.0.15",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.30",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "eslint": "^8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.31.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,htm,html}": [
      "eslint --fix --cache",
      "stylelint --fix --cache"
    ],
    "*.{js,jsx,ts,tsx,json,yml,yaml,htm,html}": "eslint --fix --cache",
    "*.{css,sass,scss,htm,html}": "stylelint --fix --cache",
    "*": "prettier -w -u"
  }
}
